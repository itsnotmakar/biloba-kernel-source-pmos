/* flashlight start */
&pio {
	flashlights_cust_pins_en_low: flashlights_cust_pins_en_output_low@gpio152 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO152__FUNC_GPIO152>;
			output-low;
		};
	};
	flashlights_cust_pins_en_high: flashlights_cust_pins_en_output_high@gpio152 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO152__FUNC_GPIO152>;
			output-high;
		};
	};
	flashlights_cust_pins_strobe_low: flashlights_cust_pins_strobe_output_low@gpio153 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO153__FUNC_GPIO153>;
			output-low;
		};
	};
	flashlights_cust_pins_strobe_high: flashlights_cust_pins_strobe_output_high@gpio153 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO153__FUNC_GPIO153>;
			output-high;
		};
	};

};

&flashlights_cust_gpio {
	pinctrl-names = "flashlights_cust_pins_en_low", "flashlights_cust_pins_en_high",
					"flashlights_cust_pins_strobe_low", "flashlights_cust_pins_strobe_high";
	pinctrl-0 = <&flashlights_cust_pins_en_low>;
	pinctrl-1 = <&flashlights_cust_pins_en_high>;
	pinctrl-2 = <&flashlights_cust_pins_strobe_low>;
	pinctrl-3 = <&flashlights_cust_pins_strobe_high>;
	status = "okay";
};
/* flashlight end */

